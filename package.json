{
  "name": "clean-facebook",
  "version": "1.0.0-beta",
  "description": "Chrome Plugin to remove unwanted facebook content (sponsored/suggested posts)",
  "private": false,
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack --config webpack-build.config.js && npm run cleanCssJs",
    "prewatch": "npm run cleanDist",
    "prebuild": "npm run cleanDist",
    "cleanCssJs": "rm dist/popup-style.js dist/content-style.js",
    "cleanDist": "rm -rf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@types/sass": "^1.43.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
