{
  "name": "clean-facebook",
  "version": "2.0.1",
  "description": "Chrome Plugin to remove unwanted facebook content (sponsored/suggested posts)",
  "private": false,
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack --config webpack-build.config.js && npm run cleanCssJs",
    "prewatch": "npm run cleanDist",
    "prebuild": "npm run cleanDist",
    "cleanCssJs": "rm dist/popup-style.js dist/content-style.js",
    "cleanDist": "rm -rf dist",
    "bump-next-major": "node ./scripts/bump-version.js major next",
    "bump-next-minor": "node ./scripts/bump-version.js minor next",
    "bump-next-patch": "node ./scripts/bump-version.js patch next",
    "bump-release": "node ./scripts/bump-version.js release"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@types/chrome": "^0.0.190",
    "@types/sass": "^1.43.1",
    "@types/semver": "^7.3.12",
    "@types/yargs": "^17.0.11",
    "babel-loader": "^8.2.5",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "semver": "^7.6.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.79.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^17.5.1"
  }
}
